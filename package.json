{
  "name": "@naeemark/joi-to-swagger",
  "version": "0.1.2",
  "description": "generate swagger.json file from joi schema file",
  "keywords": [
    "swagger",
    "joi",
    "converter"
  ],
  "homepage": "https://github.com/naeemark/joi-to-swagger",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "test": "joi-to-swagger -v ./test/validator.js -h ./test/header.json -o ./test/swagger.json",
    "test-recursive": "joi-to-swagger -r -v ./test/ -h ./test/header.json -o ./test/swagger.json",
    "help": "joi-to-swagger --help"
  },
  "author": "Naeem",
  "preferGlobal": true,
  "main": "./index.js",
  "bin": {
    "joi-to-swagger": "./index.js"
  },
  "dependencies": {
    "fs-extra": "^6.0.1",
    "glob": "^7.1.2",
    "joi": "^14.3.1",
    "joi-phone-number": "^2.0.9",
    "joi-to-swagger": "^3.0.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {}
}
